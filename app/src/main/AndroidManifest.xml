<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.gerrish.mflmanagerfree"
    android:versionCode="5"
    android:versionName="1.05" >

    <uses-sdk
        android:minSdkVersion="8"  
        android:targetSdkVersion="18" />

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>

    <application
        android:allowBackup="true"
        android:icon="@drawable/mfl_icon"
        android:label="@string/app_name"
        android:theme="@style/MyTheme" >
        
        <activity android:name="com.gerrish.mflmanagerfree.HomeListActivity" android:label="@string/app_name" android:logo="@drawable/mfl_icon">
                        <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
            
        </activity>
        
        <activity android:name="com.gerrish.mflmanagerfree.MainListActivity" android:label="@string/app_name">

        </activity>
        <activity android:name="com.gerrish.mflmanagerfree.WebViewActivity" android:label="@string/app_name">
        </activity>
        <activity android:label="@string/app_name" android:name="com.gerrish.mflmanagerfree.LogInListActivity"></activity>
        <activity android:label="@string/app_name" android:name="com.gerrish.mflmanagerfree.StandingsListActivity"></activity>
        <activity android:name="com.gerrish.mflmanagerfree.SQLiteGetPlayers"></activity>
        <activity android:name="com.gerrish.mflmanagerfree.SQLiteRosters"></activity>
        <activity android:name="com.gerrish.mflmanagerfree.LiveScoringActivity"></activity>
        
    </application>

</manifest>
